
all: gstsharkserver gstsharkclient

gstsharkserver: gstsharkserver.c
	gcc -Wall -g gstsharkserver.c -o gstsharkserver  -pthread -I/usr/include/gstreamer-1.0 -I/usr/include/glib-2.0 -I/usr/lib/x86_64-linux-gnu/glib-2.0/include  -L/usr/lib/x86_64-linux-gnu -lgstreamer-1.0 -lgobject-2.0 -lglib-2.0

gstsharkclient: gstsharkclient.c
	gcc -Wall -g gstsharkclient.c -o gstsharkclient  -pthread -I/usr/include/gstreamer-1.0 -I/usr/include/glib-2.0 -I/usr/lib/x86_64-linux-gnu/glib-2.0/include  -L/usr/lib/x86_64-linux-gnu -lgstreamer-1.0 -lgobject-2.0 -lglib-2.0

clean:
	rm -f gstsharkserver gstsharkclient
